.nav
  ul.nav-list
    each value, key in theme.menu
      li.nav-list-item
            - var re = /^(http|https):\/\/*/gi;
            - var tar = re.test(value) ? "_blank" : "_self"
            a.nav-list-link(class={active: (is_home() && value === "/") || (is_archive() && value === "/archives") } href=value target=tar)
                != key.toUpperCase()
  ul.icon-nav-list
    each value, key in theme.iconmenu
      li.icon-nav-list-item
        a.iconfont(class="icon-"+key target="_blank" href=value)
